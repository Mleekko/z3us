{
	"extends": "tsconfig/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@src/*": ["src/*"],
			"rxjs": ["node_modules/rxjs"],
			"rxjs/*": ["node_modules/rxjs/*"]
		},
	},
	"include": [
		"package.json",
		"manifest.ts",
		"vite.config.ts",
		"playwright.config.ts",
		"src/**/*.d.ts",
		"tests/**/*.ts",
		"src/**/*.ts",
		"src/**/*.tsx"
	]
}
