{
	"name": "z3us-website",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
		"dev": "concurrently \"contentlayer dev\" \"next dev -p 3001\"",
		"build": "contentlayer build && next build",
		"build:serve": "serve -p 3001 ./out",
		"lint": "next lint",
		"start": "next start",
		"deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll",
		"postbuild": "next-sitemap"
	},
	"dependencies": {
		"@radixdlt/radix-dapp-toolkit": "^0.2.0",
		"@tanstack/react-query": "^4.7.1",
		"@tanstack/react-query-devtools": "^4.7.1",
		"clsx": "^1.2.1",
		"immer": "^10.0.2",
		"@vanilla-extract/css": "^1.11.1",
		"@vanilla-extract/next-plugin": "^2.1.3",
		"concurrently": "^8.2.0",
		"contentlayer": "^0.3.3",
		"framer-motion": "^10.12.16",
		"next": "13.4.7",
		"next-contentlayer": "^0.3.3",
		"next-sitemap": "^4.1.3",
		"next-themes": "^0.2.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "^6.13.0",
		"ui": "*"
	},
	"devDependencies": {
		"@types/node": "20.3.1",
		"@types/react": "18.2.13",
		"@types/react-dom": "18.2.6",
		"@types/eslint": "^8.40.2",
		"eslint": "^8.43.0",
		"concurrently": "^8.2.0",
		"typescript": "5.0.4"
	}
}
